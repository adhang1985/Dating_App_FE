{
  "expo": {
    "name": "Quickies",
    "slug": "quickies-dating-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1B5EBD"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to capture and upload your profile photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select and upload your profile photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1B5EBD"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}